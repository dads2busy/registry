@model IList<Questionnaire2.Models.Response>

@{
    ViewBag.Title = "Download";
}

<h2>My IMPACT</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Downloads</legend>
        <p>
            <input type="submit" id="btnWord" name="Command" value="MS Word" />
            <input type="submit" id="btnPdf" name="Command" value="Pdf" />
            <input type="submit" id="btnCert" name="Command" value="Certificate" />
        </p>
    </fieldset>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
